    [% END %]    
    
    [% IF (prym == "kaukc") and $tekriadok==$row.mrecnum %]
        <th id=c5[% keymove %]><a style="color:#ff0000; font-size:14pt;" href="/cgi-bin/cleankau.cgi?idkrt=[% num1 %]&fbase=1&id=[% row.mrecnum %]&redag=rkaukc&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" title=[% zakplat %] id=p5[% keymove %]  class="cleankau">clean</a></th>
