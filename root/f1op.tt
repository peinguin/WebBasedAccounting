    [% IF (prym == "sum") and $tekriadok==$row.mrecnum %]
    
        [% IF $row.msummaop == 0 %]
            <td id=c6[% keymove %]><input type=text onblur="this.focus()" id="vvod" size=9 name="parval"></td>
        [% ELSE %]
            <td id=c6[% keymove %]><input type=text onblur="this.focus()" id="vvod" size=9 value='.sprintf("%9.2f",$row.msummaop) %] name="parval"></td>
        [% END %]        <input type=hidden  name=id value=[% row.mrecnum %]>
        <input type=hidden  name=redag value="summa">
    [% ELSE %]
        [% IF $row.msummaop<0 %]
            <th id=c6[% keymove %]><a style="color:#ff0000;" href="/cgi-bin/f1op.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=sum&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" id=p6[% keymove %]  class="redf1op">[% row.msummaop %]</a></th>
        [% ELSE %]
            <th id=c6[% keymove %]><a href="/cgi-bin/f1op.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=sum&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" id=p6[% keymove %]  class="redf1op">[% row.msummaop %]</a></th>
        [% END %]    [% END %]    
    [% IF $row.mprizop == "" %]
        [% row.mprizop = "-" %]
    [% END %]    [% IF (prym == "priznak") and $tekriadok==$row.mrecnum %]
        [% IF $row.mprizop == "-" %]
            <td id=c7[% keymove %]><input type=text size=23 onblur="this.focus()" id="vvod" name="parval"></td>
        [% ELSE %]
            <td id=c7[% keymove %]><input type=text size=23 onblur="this.focus()" id="vvod" value="[% row.mprizop %]" name="parval"></td>
        [% END %]    
        <input type=hidden  name=id value=[% row.mrecnum %]>
        <input type=hidden  name=redag value="priznak">
    [% ELSE %]
        <th id=c7[% keymove %] align="left"><a href="/cgi-bin/f1op.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=priznak&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" id=p7[% keymove %]  class="redf1op">[% row.mprizop %]</a></th>
    [% END %]    
    <td bgcolor="#ddeedd">[% row.satel %]</td>
    
    <td bgcolor="#ddeedd"><a href="/cgi-bin/finf1opdel.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=[% prym %]&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" id='p[% keymove %]'  class="delrriad"> Удалить</td></tr>
    
    <script type="text/javascript">
        document.getElementById("vvod").focus()
    </script>
[% END %]

