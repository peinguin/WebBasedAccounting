[% keymove = 0 %]

[% FOREACH row IN all %]
    
    [% keymove = keymove + 1 %]
    
    <tr id='t[% keymove %]' ><td align=left>

    [% zakplat   = row.mzakname   %]
    [% zakplatkc = row.mzaknamekc %]
    
    <a href="/cgi-bin/findublf1op.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=[% prym %]&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]&komm2=[% row.mzakname %]"  class="riaddubl">+</a></td>
    
    
    [% IF (prym == "oper") and row.tekriadok == row.mrecnum %]
        <td id=c1[% keymove %] bgcolor="#ffffff"><FONT COLOR="#ff0000">[% row.mnameop %]</FONT></td>
    [% else %]
        <th id=c1[% keymove %] align="left"><a href="/cgi-bin/f1op.cgi?idkrt=[% row.num1 %]&id=[% row.mrecnum %]&redag=oper&psort=[% row.idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" title=[% zakplat %] id=p1[% keymove %] class="redoper">[% row.mnameop %]</a></th>
    [% END %]

    [% IF row.mdok == "" %]
        [% row.mdok = "0" %]
    [% END %]    
    [% IF (prym == "dok") and $tekriadok==$row.mrecnum %]
        [% IF row.mdok == 0 %]
            <td id=c2[% keymove %]><input type=text size=7 onblur="this.focus()" id="vvod" name="parval"></td>
        [% ELSE %]
            <td id=c2[% keymove %]><input type=text size=7 value=[% row.mdok %] onblur="this.focus()" id="vvod" name="parval"></td>
        [% END %]        <input type=hidden  name=id value=[% row.mrecnum %]>
        <input type=hidden  name=redag value="dok">
    [% ELSE %]
        <th id=c2[% keymove %]><a href="/cgi-bin/f1op.cgi?idkrt=[% num1 %]&id=[% row.mrecnum %]&redag=dok&psort=[% idsort %]&navnakl=[% sdvigpn + 1 %]&sprnum=[% msp %]" id=p2[% keymove %]  class="redf1op">[% row.mdok %]</a></th>
    [% END %]    
    [% IF (prym == "data") and $tekriadok==$row.mrecnum %]
